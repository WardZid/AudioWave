{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "AudioWave.UsersService",
    "Audience": "AudioWave.API",
    "Secret": "JWT_SECRET_PLACEHOLDER"
  },
  "RabbitMQ": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "ConsumerQueue": "AudioFileQueue",
    "ClientProvidedName": "AudioFileService"
  },
  "AWS": {
    "Profile": "audiofile-s3-profile",
    "Region": "eu-north-1"
  }
}
